{
    "name": "stillat/blade-parser",
    "autoload": {
        "psr-4": {
            "Stillat\\BladeParser\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stillat\\BladeParser\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.2.0",
        "laravel/framework": "^10.40  || ^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "*",
        "laravel/pint": "^1.4",
        "brianium/paratest": "*",
        "pestphp/pest": "^3.3"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Stillat\\BladeParser\\ServiceProvider",
                "Stillat\\BladeParser\\Providers\\ValidatorServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
