{
    "name": "stillat/blade-parser",
    "autoload": {
        "psr-4": {
            "Stillat\\BladeParser\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stillat\\BladeParser\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1",
        "laravel/framework": "^9.36 || ^10.0  || ^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.0 || ^10.0",
        "orchestra/testbench": "*",
        "laravel/pint": "^1.4",
        "brianium/paratest": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Stillat\\BladeParser\\ServiceProvider",
                "Stillat\\BladeParser\\Providers\\ValidatorServiceProvider"
            ]
        }
    }
}
